{
  "name": "ragprj",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "dev": "ts-node-dev --respawn --transpile-only src/bin/www.ts",
    "build": "tsc -p tsconfig.json",
    "start:all": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "start-backend": "npm run dev",
    "start-frontend": "cd ui-angular && ng serve"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongodb": "^6.20.0",
    "morgan": "~1.9.1",
    "multer": "^2.0.2",
    "openai": "^6.3.0",
    "pdf2json": "^4.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/mongodb": "^4.0.7",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.26",
    "axios": "^1.13.2",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
